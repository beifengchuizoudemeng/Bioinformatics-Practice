define("wiki-common:widget/ui/flip/flip",function(t,i,e){function s(t){var t=t||{};this.$flipHost=t.flipHost,this.$front=this.$flipHost.children(".wgt-flip-front"),this.$back=this.$flipHost.children(".wgt-flip-back"),this.$flipHost.css(o.extend({position:"static"==this.$flipHost.css("position")?"relative":this.$flipHost.css("position")},f&&r&&l&&u?{transformStyle:"preserve-3d"}:{})),this.$front.css(o.extend({position:"absolute",left:0,top:0,width:"100%",height:"100%"},f&&r&&l&&u?{backfaceVisibility:"hidden"}:{})),this.$back.css(o.extend({position:"absolute",left:0,top:0,width:"100%",height:"100%"},f&&r&&l&&u?{backfaceVisibility:"hidden",transform:"rotateY(180deg)"}:{opacity:0})),this.orientation=/^VER|HOR$/i.test(t.orientation)?RegExp.$_.toUpperCase():"HOR",this.rotate="HOR"==this.orientation?"rotateY":"rotateX",this.duration="number"==typeof t.duration?t.duration:500,this.easing="string"==typeof t.easing?t.easing:"linear",this.callback="function"==typeof t.callback?t.callback:function(){},this.ifUseQueue="boolean"==typeof t.ifUseQueue?t.ifUseQueue:!0,this.flipStatus="STOP",this.flipQueue=[]}function a(t,i,e,s){if("FLIP"==t)var i=(this[this.rotate]||0)+i;var o=this[this.rotate]||0,n=i-o,c=this;this.flipAni=p({duration:e,easing:s,onStep:function(t){if(f&&r&&l&&u){c.$flipHost.css({transform:"perspective(1280px) "+c.rotate+"("+(o+t*n)+"deg)"});var i=c.$flipHost.attr("style");c[c.rotate]=parseFloat("HOR"==c.orientation?i.match(/rotateY\(-?\d*\.?\d*e?-?\d*deg\)/)[0].replace(/[a-z]|\(|\)/gi,""):i.match(/rotateX\(-?\d*\.?\d*e?-?\d*deg\)/)[0].replace(/[a-z]|\(|\)/gi,""))}else c.$front.css({opacity:Math.abs(180-o-t*n)%360/180}),c.$back.css({opacity:Math.abs(o+t*n)%360/180}),c[c.rotate]=180*parseFloat(c.$back.css(h))},onComplete:function(){c.flipQueue.shift(),c.flipQueue.length>0?a.apply(c,c.flipQueue[0]):c.flipStatus="STOP",c.callback()}})}var o=t("wiki-common:widget/lib/jquery/jquery"),n=t("wiki-common:widget/util/checkCSS"),p=t("wiki-common:widget/util/animation"),f=n("tra