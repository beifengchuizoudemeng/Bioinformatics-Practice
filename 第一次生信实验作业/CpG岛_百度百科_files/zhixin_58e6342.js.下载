define("wiki-lemma:widget/lemma_content/configModule/zhixin/zhixin",function(i,e,a){var t=i("wiki-common:widget/lib/jquery/jquery"),s=i("wiki-common:widget/lib/jsmart/jsmart"),n=i("wiki-common:widget/component/nslog/nslog"),l=new s('{%foreach $dataList as $card => $list%}\n<div class="zhixin-group js-group" data-dataType="{%$dataType|f_escape_xml%}">\n<h6 class="js-cardTitle">\n<span>{%$list.tipTitle|f_escape_xml%}</span>\n</h6>\n<div class="zhixin-fixBtn js-fixBtn">\n<div class="openFix" title="若推荐内容有误，请点击纠错进行反馈">纠错</div>\n<div class="closeFix">关闭纠错</div>\n</div>\n<div class="zhixin-list">\n{%foreach $list.data as $key => $item%}\n<div class="zhixin-item" nslog="area" nslog-type="10000101" nslog-params=\'{"source":"{%$list.source|escape:js|f_escape_xml%}","url":"{%$item.url|escape:js|f_escape_xml%}","targetId":"{%$item.lemmaId|escape:js|f_escape_xml%}"}\'>\n<div class="portraitbox">\n<a href="{%$item.url|f_escape_xml%}" target="_blank">\n{%if !!$item.pic%}\n<img src="{%$item.pic|wiki_common_https_trans|f_escape_xml%}" alt="{%$item.title|f_escape_xml%}" />\n{%else%}\n<img src="https://bkssl.bdimg.com/static/wiki-lemma/widget/lemma_content/configModule/zhixin/img/cityzhixinemptyimg_498f02b.png" alt="{%$item.title|f_escape_xml%}" />\n{%/if%}\n</a>\n</div>\n<p class="item-title js-name"><a href="{%$item.url|f_escape_xml%}" target="_blank" title="{%$item.title|f_escape_xml%}">{%$item.title|f_escape_xml%}</a></p>\n<div class="zhixinErrorBox js-fixBox hidden" data-lemma-id="{%$item.lemmaId|f_escape_xml%}">\n<a class="submit-trigger" href="javascript:;">有错误</a>\n<span class="js-text hidden">已反馈</span>\n</div>\n</div>\n{%/foreach%}\n</div>\n</div>\n{%/foreach%}');a.exports=function(i,e){function a(){var i=t("#zhixinWrap"),a=t("#zhixinErrorForm"),s=a.find(".js-url"),l=a.find(".js-query"),m=a.find(".js-title"),r=a.find(".js-content"),o=a.find(".js-souceId");i.on("click",".js-fixBtn",function(){var i=t(this);i.toggleClass("show"),i.parent(".js-group").find(".js-fixBox").toggle